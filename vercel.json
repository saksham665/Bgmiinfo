{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/fetch",
      "methods": ["GET"],
      "dest": "/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ],
  "functions": {
    "api.py": {
      "maxDuration": 10
    }
  }
}
